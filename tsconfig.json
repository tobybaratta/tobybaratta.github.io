{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "baseUrl": ".",
      "types": ["vitepress/client"]

    },
  "include": [
    "docs/.vitepress/**/*.ts",
    "docs/.vitepress/**/*.vue",
    "vite.config.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "docs/.vitepress/cache"
  ]
}
